{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
        "entities": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "uniqueItems": true,
            "minItems": 2,
            "maxItems": 2
        },
        "maxDistance": {
            "type": "integer"
        }
    },
    "required": ["entities", "maxDistance"]
}